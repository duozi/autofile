<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:beans="http://www.springframework.org/schema/beans"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
  http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       default-autowire="byName">

    <dubbo:application name="dubbo_test_zhouxi" owner="zhouxi.zhou" organization="com.qunar.fresh"/>

    <!--&lt;!&ndash; #################### galigeo ##################&ndash;&gt;-->
    <dubbo:registry address="${registry.address}"
                    protocol="zookeeper"
                    group="abacusproxy_betab"
                    timeout="6000"
                    id="FuwugalileoRegistry"/>

    <dubbo:reference id="gdsProxyService"
                     interface="com.qunar.fuwu.gdsproxy.api.GdsProxyService"
                     timeout="30000"
                     check="false"
                     version="1.0.0"
                     loadbalance="random"
                     registry="FuwugalileoRegistry">
        <!--陆游规则与abacus一样-->
    </dubbo:reference>

</beans>
