<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd http://www.springframework.org/schema/task http://www.springframework.org/schema/task/spring-task.xsd">
    <context:property-placeholder location="classpath:source.beta/config/*.properties" ignore-unresolvable="true"/>
    <context:component-scan base-package="com.qunar.*"/>
    <context:annotation-config/>
    <import resource="classpath:source.beta/spring/qschedule.xml"/>
    <import resource="classpath:source.beta/spring/consumer.xml"/>
</beans>